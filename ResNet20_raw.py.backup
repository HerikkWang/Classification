        self.block3_bn1 = nn.BatchNorm2d(16)
        self.block3_conv2 = nn.Conv2d(16, 16, kernel_size=3, stride=1, padding=1, bias=False)
        self.block3_bn2 = nn.BatchNorm2d(16)
        self.block3_shortcut = nn.Sequential()
        # block4
