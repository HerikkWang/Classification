        self.block1_bn2 = nn.BatchNorm2d(16)
        self.block1_shortcut = nn.Sequential()
        # block2
        self.block2_conv1 = nn.Conv2d(16, 512, kernel_size=1, stride=1, bias=False)
        self.block2_bn1 = nn.BatchNorm2d(512)
        self.block2_conv2 = nn.Conv2d(512, 16, kernel_size=1, stride=1, bias=False)
        self.block2_bn2 = nn.BatchNorm2d(16)
        self.block2_shortcut = nn.Sequential()
        # block3
        self.block3_conv1 = nn.Conv2d(16, 512, kernel_size=1, stride=1, bias=False)
        self.block3_bn1 = nn.BatchNorm2d(512)
        self.block3_conv2 = nn.Conv2d(512, 16, kernel_size=1, stride=1, bias=False)
        self.block3_bn2 = nn.BatchNorm2d(16)
        self.block3_shortcut = nn.Sequential()
        # block4
        self.block4_conv1 = nn.Conv2d(16, 512, kernel_size=1, stride=1, bias=False)
        self.block4_bn1 = nn.BatchNorm2d(512)
        self.block4_conv2 = nn.Conv2d(512, 16, kernel_size=1, stride=1, bias=False)
        self.block4_bn2 = nn.BatchNorm2d(16)
        self.block4_shortcut = nn.Sequential()
        # block5
        self.block5_conv1 = nn.Conv2d(16, 512, kernel_size=1, stride=1, bias=False)
        self.block5_bn1 = nn.BatchNorm2d(512)
        self.block5_conv2 = nn.Conv2d(512, 16, kernel_size=1, stride=1, bias=False)
        self.block5_bn2 = nn.BatchNorm2d(16)
        self.block5_shortcut = nn.Sequential()
        # block6
        self.block6_conv1 = nn.Conv2d(16, 1024, kernel_size=1, stride=1, bias=False)
        self.block6_bn1 = nn.BatchNorm2d(1024)
        self.block6_conv2 = nn.Conv2d(1024, 16, kernel_size=1, stride=1, bias=False)
        self.block6_bn2 = nn.BatchNorm2d(16)
        self.block6_shortcut = nn.Sequential()
        # block7
        self.block7_conv1 = nn.Conv2d(16, 1024, kernel_size=1, stride=1, bias=False)
        self.block7_bn1 = nn.BatchNorm2d(1024)
        self.block7_conv2 = nn.Conv2d(1024, 16, kernel_size=1, stride=1, bias=False)
        self.block7_bn2 = nn.BatchNorm2d(16)
        self.block7_shortcut = nn.Sequential()
        # block8
        self.block8_conv1 = nn.Conv2d(16, 1024, kernel_size=1, stride=1, bias=False)
        self.block8_bn1 = nn.BatchNorm2d(1024)
        self.block8_conv2 = nn.Conv2d(1024, 16, kernel_size=1, stride=1, bias=False)
        self.block8_bn2 = nn.BatchNorm2d(16)
        self.block8_shortcut = nn.Sequential()
        # block9
        self.block9_conv1 = nn.Conv2d(16, 1024, kernel_size=1, stride=1, bias=False)
        self.block9_bn1 = nn.BatchNorm2d(1024)
        self.block9_conv2 = nn.Conv2d(1024, 16, kernel_size=1, stride=1, bias=False)
        self.block9_bn2 = nn.BatchNorm2d(16)
        self.block9_shortcut = nn.Sequential()
        self.linear = nn.Linear(48, num_classes)
        self.apply(_weights_init)
    
    def forward(self, x):
        out = x.reshape(-1, 3, 2, 16, 2, 16)
        out = out.permute(0, 1, 2, 4, 3, 5)
        out = out.reshape(-1, 12, 16, 16)
        out = self.conv1(out)
        out = self.bn1(out)
        out = F.relu(out)
        # block1
        shortcut1 = self.block1_shortcut(out)
        out = F.relu(self.block1_bn1(self.block1_conv1(out)))
        out = self.block1_bn2(self.block1_conv2(out))
        out += shortcut1
        out = F.relu(out)
        out = out.permute(0, 3, 1, 2)
        # block2
        shortcut2 = self.block2_shortcut(out)
        out = F.relu(self.block2_bn1(self.block2_conv1(out)))
        out = self.block2_bn2(self.block2_conv2(out))
        out += shortcut2
        out = F.relu(out)
        out = out.permute(0, 3, 1, 2)
        # block3
        shortcut3 = self.block3_shortcut(out)
        out = F.relu(self.block3_bn1(self.block3_conv1(out)))
        out = self.block3_bn2(self.block3_conv2(out))
        out += shortcut3
        out = F.relu(out)
        out = out.permute(0, 3, 1, 2)
        # block4
        shortcut4 = self.block4_shortcut(out)
        out = F.relu(self.block4_bn1(self.block4_conv1(out)))
        out = self.block4_bn2(self.block4_conv2(out))
        out += shortcut4
        out = F.relu(out)
        out = out.permute(0, 3, 1, 2)
        # block5
        shortcut5 = self.block5_shortcut(out)
        out = F.relu(self.block5_bn1(self.block5_conv1(out)))
        out = self.block5_bn2(self.block5_conv2(out))
        out += shortcut5
        out = F.relu(out)
        out = out.permute(0, 3, 1, 2)
        # block6
        shortcut6 = self.block6_shortcut(out)
        out = F.relu(self.block6_bn1(self.block6_conv1(out)))
        out = self.block6_bn2(self.block6_conv2(out))
        out += shortcut6
        out = F.relu(out)
        out = out.permute(0, 3, 1, 2)
        # block7
        shortcut7 = self.block7_shortcut(out)
        out = F.relu(self.block7_bn1(self.block7_conv1(out)))
        out = self.block7_bn2(self.block7_conv2(out))
        out += shortcut7
        out = F.relu(out)
        out = out.permute(0, 3, 1, 2)
        # block8
        shortcut8 = self.block8_shortcut(out)
        out = F.relu(self.block8_bn1(self.block8_conv1(out)))
        out = self.block8_bn2(self.block8_conv2(out))
        out += shortcut8
        out = F.relu(out)
        out = out.permute(0, 3, 1, 2)
        # block9
        shortcut9 = self.block9_shortcut(out)
        out = F.relu(self.block9_bn1(self.block9_conv1(out)))
        out = self.block9_bn2(self.block9_conv2(out))
        out += shortcut9
        out = F.relu(out)
        out = out.permute(0, 3, 1, 2)

        out1 = F.avg_pool2d(out, out.size()[3])
        out2 = F.avg_pool2d(out.permute(0, 3, 1, 2), out.size()[3])
        out3 = F.avg_pool2d(out.permute(0, 2, 3, 1), out.size()[3])
        out = torch.cat([out1, out2, out3], 1)
        # print(out.size())
        out = out.view(out.size(0), -1)
        out = self.linear(out)
        return out

class cube_block(nn.Module):
    def __init__(self, in_planes, out_planes, stride=1, kernel_size=1, padding=0, norm="BatchNorm", block_norm="BatchNorm", twist=True) -> None:
        super(cube_block, self).__init__()
        self.norm = norm
        self.twist = twist
        self.conv1 = nn.Conv2d(in_planes, out_planes, kernel_size=kernel_size, stride=stride, padding=padding, bias=False)
        self.conv2 = nn.Conv2d(in_planes, out_planes, kernel_size=kernel_size, stride=stride, padding=padding, bias=False)
        self.conv3 = nn.Conv2d(in_planes, out_planes, kernel_size=kernel_size, stride=stride, padding=padding, bias=False)
        if norm == "BatchNorm":
            self.bn1 = nn.BatchNorm2d(out_planes)
            self.bn2 = nn.BatchNorm2d(out_planes)
            self.bn3 = nn.BatchNorm2d(out_planes)
        elif norm == "LayerNorm":
            self.bn1 = nn.LayerNorm([out_planes, out_planes, out_planes])
            self.bn2 = nn.LayerNorm([out_planes, out_planes, out_planes])
            self.bn3 = nn.LayerNorm([out_planes, out_planes, out_planes])
        else:
            pass
        if block_norm == "BatchNorm":
            self.block_norm = nn.BatchNorm2d(out_planes)
        elif block_norm == "LayerNorm":
            self.block_norm = nn.LayerNorm([out_planes, out_planes, out_planes])
        else:
            self.block_norm = nn.Sequential() 
        self.shortcut = nn.Sequential()
    
    def forward(self, x):
        shortcut = self.shortcut(x)
        if self.norm != "NoNorm":
            out = F.relu(self.bn1(self.conv1(x)))
            if self.twist:
                out = out.permute(0, 3, 1, 2)
            out = F.relu(self.bn2(self.conv2(out)))
            if self.twist:
                out = out.permute(0, 3, 1, 2)
            out = self.bn3(self.conv3(out))
            if self.twist:
                out = out.permute(0, 3, 1, 2)
            out += shortcut
            out = self.block_norm(F.relu(out))
        else:
            out = F.relu(self.conv1(x))
            if self.twist:
                out = out.permute(0, 3, 1, 2)
            out = F.relu(self.conv2(out))
            if self.twist:
                out = out.permute(0, 3, 1, 2)
            out = self.conv3(out)
            if self.twist:
                out = out.permute(0, 3, 1, 2)
            out += shortcut
            out = self.block_norm(F.relu(out))
        return out

class cube_pair_block(nn.Module):
    def __init__(self, planes, dilate_planes, stride=1, kernel_size=1, padding=0, norm="BatchNorm") -> None:
        super(cube_pair_block, self).__init__()
        self.norm = norm
        self.conv1 = nn.Conv2d(planes, dilate_planes, kernel_size=kernel_size, stride=stride, padding=padding, bias=False)
        self.conv2 = nn.Conv2d(planes, dilate_planes, kernel_size=kernel_size, stride=stride, padding=padding, bias=False)
        self.conv3 = nn.Conv2d(planes, dilate_planes, kernel_size=kernel_size, stride=stride, padding=padding, bias=False)
        self.conv4 = nn.Conv2d(dilate_planes, planes, kernel_size=kernel_size, stride=stride, padding=padding, bias=False)
        self.conv5 = nn.Conv2d(dilate_planes, planes, kernel_size=kernel_size, stride=stride, padding=padding, bias=False)
        self.conv6 = nn.Conv2d(dilate_planes, planes, kernel_size=kernel_size, stride=stride, padding=padding, bias=False)
        if norm == "BatchNorm":
            self.bn1 = nn.BatchNorm2d(dilate_planes)
            self.bn2 = nn.BatchNorm2d(dilate_planes)
            self.bn3 = nn.BatchNorm2d(dilate_planes)
            self.bn4 = nn.BatchNorm2d(planes)
            self.bn5 = nn.BatchNorm2d(planes)
            self.bn6 = nn.BatchNorm2d(planes)
        elif norm == "LayerNorm":
            self.bn1 = nn.LayerNorm([dilate_planes, planes, planes])
            self.bn2 = nn.LayerNorm([dilate_planes, dilate_planes, planes])
            self.bn3 = nn.LayerNorm([dilate_planes, dilate_planes, dilate_planes])
            self.bn4 = nn.LayerNorm([planes, dilate_planes, dilate_planes])
            self.bn5 = nn.LayerNorm([planes, planes, dilate_planes])
            self.bn6 = nn.LayerNorm([planes, planes, planes])
        else:
            pass
        self.shortcut = nn.Sequential()
    
    def forward(self, x):
        shortcut = self.shortcut(x)
        if self.norm != "NoNorm":
            out = F.relu(self.bn1(self.conv1(x)))
            out = out.permute(0, 3, 1, 2)
            out = F.relu(self.bn2(self.conv2(out)))
            out = out.permute(0, 3, 1, 2)
            out = F.relu(self.bn3(self.conv3(out)))
            out = out.permute(0, 3, 1, 2)
            out = F.relu(self.bn4(self.conv4(out)))
            out = out.permute(0, 3, 1, 2)
            out = F.relu(self.bn5(self.conv5(out)))
            out = out.permute(0, 3, 1, 2)
            out = self.bn6(self.conv6(out))
            out = out.permute(0, 3, 1, 2)
            out += shortcut
            out = F.relu(out)
        else:
            out = F.relu(self.conv1(x))
            out = out.permute(0, 3, 1, 2)
            out = F.relu(self.conv2(out))
            out = out.permute(0, 3, 1, 2)
            out = F.relu(self.conv3(out))
            out = out.permute(0, 3, 1, 2)
            out = F.relu(self.conv4(out))
            out = out.permute(0, 3, 1, 2)
            out = F.relu(self.conv5(out))
            out = out.permute(0, 3, 1, 2)
            out = self.conv6(out)
            out = out.permute(0, 3, 1, 2)
            out += shortcut
            out = F.relu(out)
        return out

class cube_downsample(nn.Module):
    def __init__(self, in_planes, out_planes, kernel_size=1, stride=1, padding=0, norm='LayerNorm') -> None:
        super().__init__()
        self.conv1 = nn.Conv2d(in_planes, out_planes, kernel_size=kernel_size, stride=stride, padding=padding)
        self.conv2 = nn.Conv2d(in_planes, out_planes, kernel_size=kernel_size, stride=stride, padding=padding)
        self.conv3 = nn.Conv2d(in_planes, out_planes, kernel_size=kernel_size, stride=stride, padding=padding)
        self.norm = norm
        if norm == "LayerNorm":
            self.norm_layer = nn.LayerNorm([out_planes, out_planes, out_planes])
        elif norm == "BatchNorm":
            self.norm_layer = nn.BatchNorm2d(out_planes)
        else:
            pass
        # if block_norm == "BatchNorm":
        #     self.block_norm = nn.BatchNorm2d(out_planes)
        # elif block_norm == "LayerNorm":
        #     self.block_norm == nn.LayerNorm([out_planes, out_planes, out_planes])
        # else:
        #     pass
    def forward(self, x):
        out = self.conv1(x)
        out = F.relu(out.permute(0, 3, 1, 2))
        out = self.conv2(out)
        out = F.relu(out.permute(0, 3, 1, 2))
        out = self.conv3(out)
        out = F.relu(out.permute(0, 3, 1, 2))
        if self.norm != "NoNorm":
            out = self.norm_layer(out)
        
        return out

class Cube_net_16_16(nn.Module):
    def __init__(self, cube_size=16, norm="BatchNorm", num_classes=100, res_cube_size=4):
        super(Cube_net_16_16, self).__init__()
        self.cube_size = cube_size
        self.conv1 = nn.Conv2d(12, 16, kernel_size=3, stride=1, padding=1, bias=False)
        self.bn1 = nn.BatchNorm2d(16)
        self.block1 = cube_block(norm=norm, in_planes=cube_size, out_planes=16, stride=1, kernel_size=5, padding=2)
        self.block2 = cube_block(norm=norm, in_planes=cube_size, out_planes=16, stride=1, kernel_size=5, padding=2)
        self.block3 = cube_block(norm=norm, in_planes=cube_size, out_planes=16, stride=1, kernel_size=5, padding=2)
        self.block4 = cube_pair_block(norm=norm, planes=cube_size, dilate_planes=32, stride=1, kernel_size=3, padding=1)
        self.block5 = cube_pair_block(norm=norm, planes=cube_size, dilate_planes=32, stride=1, kernel_size=3, padding=1)
        # self.block6 = cube_pair_block(norm=True, planes=16, dilate_planes=32, stride=1, kernel_size=3, padding=1)
        # self.block7 = cube_pair_block(norm=True, planes=16, dilate_planes=64, stride=1, kernel_size=3, padding=1)
        self.block8 = cube_pair_block(norm=norm, planes=cube_size, dilate_planes=64, stride=1, kernel_size=3, padding=1)
        self.block9 = cube_pair_block(norm=norm, planes=cube_size, dilate_planes=64, stride=1, kernel_size=3, padding=1)
        # down sample
        self.downsample_conv1 = nn.Sequential(
            nn.Conv2d(cube_size, res_cube_size, kernel_size=1, stride=1, bias=False),
            nn.BatchNorm2d(res_cube_size),
            nn.ReLU(inplace=True)
        )
        self.downsample_conv2 = nn.Sequential(
            nn.Conv2d(cube_size, res_cube_size, kernel_size=1, stride=1, bias=False),
            nn.BatchNorm2d(res_cube_size),
            nn.ReLU(inplace=True)
        )    
        self.downsample_conv3 = nn.Sequential(
            nn.Conv2d(cube_size, res_cube_size, kernel_size=1, stride=1, bias=False),
            nn.BatchNorm2d(res_cube_size),
            nn.ReLU(inplace=True)
        )     
        # self.downsample_conv2 = nn.Conv2d(32, 4, kernel_size=1, stride=1, bias=False)
        # self.downsample_conv3 = nn.Conv2d(32, 4, kernel_size=1, stride=1, bias=False)
        # self.downsample_bn1 = nn.BatchNorm2d(4)
        # self.downsample_bn1 = nn.BatchNorm2d(4)
        # self.downsample_bn1 = nn.BatchNorm2d(4)

        self.block9_shortcut = nn.Sequential()

        self.linear = nn.Linear(res_cube_size ** 3, num_classes)
        self.apply(_weights_init)
    
    def forward(self, x):
        out = x.reshape(-1, 3, 2, 16, 2, 16)
        out = out.permute(0, 1, 2, 4, 3, 5)
        out = out.reshape(-1, 12, 16, 16)
        out = self.conv1(out)
        out = self.bn1(out)
        out = F.relu(out)
        out = self.block1(out)
        out = self.block2(out)
        out = self.block3(out)
        out = self.block4(out)
        out = self.block5(out)
        # out = self.block6(out)
        # out = self.block7(out)
        out = self.block8(out)
        out = self.block9(out)
        out = self.downsample_conv1(out)
        out = out.permute(0, 3, 1, 2)
        out = self.downsample_conv2(out)
        out = out.permute(0, 3, 1, 2)
        out = self.downsample_conv3(out)
        out = out.permute(0, 3, 1, 2)


        # out1 = F.avg_pool2d(out, out.size()[3])
        # out2 = F.avg_pool2d(out.permute(0, 3, 1, 2), out.size()[3])
        # out3 = F.avg_pool2d(out.permute(0, 2, 3, 1), out.size()[3])
        # out = torch.cat([out1, out2, out3], 1)
        # print(out.size())

        out = out.reshape(out.size(0), out.size(1)*out.size(2)*out.size(3))
        out = self.linear(out)
        return out

