        self.block5_conv3 = nn.Conv2d(64, 32, kernel_size=1, stride=1, bias=False)
        # self.block5_conv4 = nn.Conv2d(128, 32, kernel_size=1, stride=1, bias=False)
        # self.block5_conv5 = nn.Conv2d(32, 128, kernel_size=1, stride=1, bias=False)
        # self.block5_conv6 = nn.Conv2d(128, 32, kernel_size=1, stride=1, bias=False)
        self.block5_shortcut = nn.Sequential()
        # block6
        self.block6_conv1 = nn.Conv2d(32, 64, kernel_size=1, stride=1, bias=False)
        self.block6_conv2 = nn.Conv2d(32, 64, kernel_size=1, stride=1, bias=False)
        self.block6_conv3 = nn.Conv2d(32, 64, kernel_size=1, stride=1, bias=False)
        # self.block6_conv4 = nn.Conv2d(128, 32, kernel_size=1, stride=1, bias=False)
        # self.block6_conv5 = nn.Conv2d(32, 128, kernel_size=1, stride=1, bias=False)
        # self.block6_conv6 = nn.Conv2d(128, 32, kernel_size=1, stride=1, bias=False)
        self.block6_shortcut = nn.Sequential()
        # block7
        self.block7_conv1 = nn.Conv2d(64, 32, kernel_size=1, stride=1, bias=False)
        self.block7_conv2 = nn.Conv2d(64, 32, kernel_size=1, stride=1, bias=False)
        self.block7_conv3 = nn.Conv2d(64, 32, kernel_size=1, stride=1, bias=False)
        # self.block7_conv4 = nn.Conv2d(256, 32, kernel_size=1, stride=1, bias=False)
        # self.block7_conv5 = nn.Conv2d(32, 256, kernel_size=1, stride=1, bias=False)
        # self.block7_conv6 = nn.Conv2d(256, 32, kernel_size=1, stride=1, bias=False)
        self.block7_shortcut = nn.Sequential()
        # block8
        self.block8_conv1 = nn.Conv2d(32, 128, kernel_size=1, stride=1, bias=False)
        self.block8_conv2 = nn.Conv2d(32, 128, kernel_size=1, stride=1, bias=False)
        self.block8_conv3 = nn.Conv2d(32, 128, kernel_size=1, stride=1, bias=False)
        # self.block8_conv4 = nn.Conv2d(256, 32, kernel_size=1, stride=1, bias=False)
        # self.block8_conv5 = nn.Conv2d(32, 256, kernel_size=1, stride=1, bias=False)
        # self.block8_conv6 = nn.Conv2d(256, 32, kernel_size=1, stride=1, bias=False)
        self.block8_shortcut = nn.Sequential()
        # block9
        self.block9_conv1 = nn.Conv2d(128, 32, kernel_size=1, stride=1, bias=False)
        self.block9_conv2 = nn.Conv2d(128, 32, kernel_size=1, stride=1, bias=False)
        self.block9_conv3 = nn.Conv2d(128, 32, kernel_size=1, stride=1, bias=False)
        # self.block9_conv4 = nn.Conv2d(256, 32, kernel_size=1, stride=1, bias=False)
        # self.block9_conv5 = nn.Conv2d(32, 256, kernel_size=1, stride=1, bias=False)
        # self.block9_conv6 = nn.Conv2d(256, 32, kernel_size=1, stride=1, bias=False)
        self.block9_shortcut = nn.Sequential()

        self.linear = nn.Linear(96, num_classes)
        self.apply(_weights_init)
    
    def forward(self, x):
        out = self.conv1(x)
        out = self.bn1(out)
        out = F.relu(out)
        # block1
        shortcut1 = self.block1_shortcut(out)
        out = F.relu(self.block1_conv1(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block1_conv2(out))
        out = out.permute(0, 3, 1, 2)
        out = self.block1_conv3(out)
        out = out.permute(0, 3, 1, 2)
        out += shortcut1
        out = F.relu(out)
        # block2
        shortcut2 = self.block2_shortcut(out)
        out = F.relu(self.block2_conv1(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block2_conv2(out))
        out = out.permute(0, 3, 1, 2)
        out = self.block2_conv3(out)
        out = out.permute(0, 3, 1, 2)
        out += shortcut2
        out = F.relu(out)
        # block3
        shortcut3 = self.block3_shortcut(out)
        out = F.relu(self.block3_conv1(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block3_conv2(out))
        out = out.permute(0, 3, 1, 2)
        out = self.block3_conv3(out)
        out = out.permute(0, 3, 1, 2)
        out += shortcut3
        out = F.relu(out)
        # block4
        shortcut4 = self.block4_shortcut(out)
        out = F.relu(self.block4_conv1(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block4_conv2(out))
        # out = F.relu(self.block4_conv4(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block4_conv3(out))
        # out = self.block4_conv6(out)
        out = out.permute(0, 3, 1, 2)
        # block5
        out = F.relu(self.block5_conv1(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block5_conv2(out))
        out = out.permute(0, 3, 1, 2)
        out = self.block5_conv3(out)
        out = out.permute(0, 3, 1, 2)
        out += shortcut4
        out = F.relu(out)
        # block6
        shortcut6 = self.block6_shortcut(out)
        out = F.relu(self.block6_conv1(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block6_conv2(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block6_conv3(out))
        out = out.permute(0, 3, 1, 2)
        # block7
        # shortcut7 = self.block7_shortcut(out)
        out = F.relu(self.block7_conv1(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block7_conv2(out))
        out = out.permute(0, 3, 1, 2)
        out = self.block7_conv3(out)
        out = out.permute(0, 3, 1, 2)
        out += shortcut6
        out = F.relu(out)
        # block8
        shortcut8 = self.block8_shortcut(out)
        out = F.relu(self.block8_conv1(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block8_conv2(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block8_conv3(out))
        out = out.permute(0, 3, 1, 2)
        # block9
        out = F.relu(self.block9_conv1(out))
        out = out.permute(0, 3, 1, 2)
        out = F.relu(self.block9_conv2(out))
        out = out.permute(0, 3, 1, 2)
        out = self.block9_conv3(out)
        out = out.permute(0, 3, 1, 2)
        out += shortcut8
        out = F.relu(out)

        out1 = F.avg_pool2d(out, out.size()[3])
        out2 = F.avg_pool2d(out.permute(0, 3, 1, 2), out.size()[3])
        out3 = F.avg_pool2d(out.permute(0, 2, 3, 1), out.size()[3])
        out = torch.cat([out1, out2, out3], 1)
        # print(out.size())
        out = out.view(out.size(0), -1)
        out = self.linear(out)
        return out

class Cube_net_conv1(nn.Module):
    def __init__(self, num_classes=10):
