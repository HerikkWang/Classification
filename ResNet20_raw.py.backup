        out = self.block5_bn2(out)
        out = torch.permute(out, position_coding("NCWH", "NHCW"))
        out += shortcut5
        out = F.relu(out)
        out = torch.permute(out, position_coding("NHCW", "NCWH"))
        # out = out.permute(0, 2, 3, 1)
        # block6
